<div class="container">
<h2><%= @event.title %></h2>
<h6>Organized by <%= link_to organizer(@event).username, organizer(@event)%> </h6>
<p><%= @event.body %></p>
  <h5>
    List of Attendees
  </h5>
  <ul>
    <% @attendee_list.each do |user| %>
      <li><%= link_to user.username, user %> </li>
    <% end %>
  </ul>
</div>
